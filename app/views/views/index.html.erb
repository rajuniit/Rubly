<h1>Listing views</h1>

<table>
  <tr>
    <th>References</th>
    <th>Ip address</th>
    <th>Browser</th>
    <th>Referer</th>
    <th>Platform</th>
    <th>Country</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @views.each do |view| %>
  <tr>
    <td><%= view.references %></td>
    <td><%= view.ip_address %></td>
    <td><%= view.browser %></td>
    <td><%= view.referer %></td>
    <td><%= view.platform %></td>
    <td><%= view.country %></td>
    <td><%= link_to 'Show', view %></td>
    <td><%= link_to 'Edit', edit_view_path(view) %></td>
    <td><%= link_to 'Destroy', view, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New View', new_view_path %>
